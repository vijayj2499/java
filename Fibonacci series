
public class Main
{   
  static void fib(int n)
    {
    int first=0,second=1,arr[],i;
    arr = new int[20];  // allocating memory to array

    arr[0]=first;
    arr[1]=second;
    for(i=2;i<n;i++)
    {
        first=first+second;
        
        arr[i]=first;
      second=arr[i-1];
       
        
    }
    for(i=0;i<n;i++)
    System.out.print(arr[i]+" ");
        
}
	public static void main(String[] args) {
fib(10);    
	
		
	
}
}
